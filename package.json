    "docker:iam": "powershell -ExecutionPolicy Bypass -File ./run-docker-iam.ps1",
    "docker:auto": "powershell -ExecutionPolicy Bypass -File ./run-docker-auto-restart.ps1"
  },
  "keywords": [
    "litellm",
    "aws",
    "bedrock",
    "claude",
    "anthropic",
    "proxy",
